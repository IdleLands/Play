{
  "name": "IdleLands",
  "author": "IdleLands <hi@idle.land>",
  "homepage": "http://idle.land",
  "version": "0.11.0",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "npm run ionic:build",
    "build:prod": "npm run ionic:build:prod",
    "ionic:build": "ionic-app-scripts build",
    "ionic:build:prod": "ionic-app-scripts build --prod",
    "ionic:serve": "ionic-app-scripts serve --address 0.0.0.0 -p 9002",
    "ionic:serve:lab": "ionic-app-scripts serve --lab -c --address 0.0.0.0 -p 9002",
    "start": "npm run ionic:serve",
    "start:lab": "npm run ionic:serve:lab",
    "deploy:pages": "gh-pages -d www",
    "deploy": "npm run build && npm run deploy:pages",
    "deploy:prod": "npm run build:prod && npm run deploy:pages",
    "prerelease": "standard-version --no-verify",
    "release": "npm run build",
    "postrelease": "npm run deploy:pages"
  },
