  "description": "Play IdleLands in the browser",
  "repository": "https://github.com/IdleLands/Play",
  "license": "MIT",
  "scripts": {
    "build:app": "webpack -p --progress && cp src/index.html dist/index.html && echo '/*    /index.html   200' > dist/_redirects && echo `date` > dist/builtat",
    "test:lint": "eslint \"src/**/*.js\"",
    "test:ava": "ava --require babel-register",
    "test": "npm run test:lint",
    "start": "webpack-dev-server --open --content-base src/ --inline --hot --host 0.0.0.0",
    "deploy": "npm run build:app && gulp deploy"
  },
